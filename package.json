{
  "name": "ts-emeth",
  "version": "1.3.0",
  "description": "CSS Modules cli for emeth and TypeScript",
  "types": "src",
  "main": "./index",
  "license": "MIT",
  "browser": "./lib/browser.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jspears/ts-emeth.git"
  },
  "scripts": {
    "test-gen": "./bin/tse.js -p ./src/__test__/fixtures/**/*.cssm",
    "test": "$npm_execpath run test-gen && mocha -r ./src/__test__/setup.js -r ts-node/register ./src/__test__/**/*-test.ts*",
    "prepare": "tsc && $npm_execpath run test",
    "watch": "tsc -w"
  },
  "bin": {
    "tse": "./bin/tse.js"
  },
  "dependencies": {
    "@mrbuilder/plugin-emeth": "^4.0.0",
    "@types/node": "^13.11.1",
    "@types/webpack": "^4.41.10",
    "@types/webpack-env": "^1.15.1",
    "chalk": "^4.1.0",
    "chokidar": "^3.5.1",
    "css-loader": "^3.6.0",
    "emeth": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "ts-arg": "^1.0.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "postcss": "^7.0.27",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "mrbuilder": {
    "plugin": "./mrbuilder-plugin.js",
    "plugins": [
      "@mrbuilder/plugin-emeth"
    ]
  }
}
